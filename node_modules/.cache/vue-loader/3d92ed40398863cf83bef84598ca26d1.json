me\" class=\"game\" @mouseup=\"MoveCenter\">\n      <!-- <div class=\"game\"> -->\n      <div class=\"stage\">\n\n        <img src=\"../assets/img/icon.png\" alt=\"\" width=\"60px\" height=\"60px\" class=\"player\"\n          v-bind:style=\"{ top: player.top + 'px', left: player.left + 'px' }\">\n\n        <div class='cell' v-for=\"(i, key) in 450\" :key=\"key\" :class=\"{line: (i%15 === 4 || i%15 === 8 || i%15 === 12)}\"\n          @mousedown=\"Tap\" @mouseup=\"Tap\">\n        </div>\n\n\n        <!-- 魚 -->\n        <img src=\"../assets/img/webgame/fish.png\" alt=\"\" width=\"60px\" height=\"60px\" class=\"fish\"\n        v-bind:style=\"{ top: fish.top + 'px', left: fish.left + 'px' }\">\n        <!-- <img src=\"../assets/img/webgame/fish.png\" alt=\"\"> -->\n\n\n\n\n\n      </div>\n\n      <div style=\"text-align: center\">\n        <img src=\"../assets/img/webgame/rightButton.png\" alt=\"\" style=\"transform: scale(-1, 1); float: left;\"\n          class=\"controller\" @mousedown=\"MoveLeft\" @touchstart=\"MoveLeft\" v-on:keydown.left=\"MoveLeft\">\n        <img src=\"../assets/img/webgame/rightButton.png\" alt=\"\" style=\"float: right;\" class=\"controller\"\n          @mousedown=\"MoveRight\" @touchstart=\"MoveRight\" v-on:mousedown.right=\"MoveRight\">\n\n      </div>\n    </div>\n\n\n\n\n\n  </div>\n\n\n</template>\n\n<script>\n  export default {\n\n\n\n\n\n\n    data() {\n      return {\n        player: {\n          top: 250,\n          left: 120,\n\n          speed: 10\n        },\n        fish: {\n          top: 0,\n          left: 120\n        }\n      }\n    },\n    mounted() {\n\n      // var target = document.getElementById('game');\n\n      // target.addEventListener('keyup', event => {\n      //   if (event.key == 'ArrowRight') {\n      //     this.MoveRight();\n      //   } else if (event.key == 'ArrowLeft') {\n      //     this.MoveLeft();\n      //   }\n      // })\n\n\n      window.addEventListener('keydown', event => {\n        if (event.key == 'ArrowRight') {\n          this.MoveRight();\n        } else if (event.key == 'ArrowLeft') {\n          this.MoveLeft();\n        }\n      })\n\n\n      window.addEventListener('keyup', () => {\n        this.MoveCenter();\n      })\n\n\n\n    },\n    created() {\n      // document.addEventListener('keydown.left', this.MoveLeft)\n      // 時間を動かし始める\n      this.time_goes()\n    },\n    beforeDestroy() {\n      // let target = document.getElementById('game');\n      // target.removeEventListener\n    },\n    methods: {\n      Tap(e) {\n        console.log(e);\n        e.currentTarget.className = \"tap\";\n      },\n      // 時間を進める\n      time_goes() {\n        console.log(\"aa\");\n        //移動処理\n        // this.$set(this.player, \"top\", this.player.top + 2)\n        this.$set(this.fish, \"top\", this.fish.top + 2)\n        // this.top += 10;\n\n        // speedミリ秒後に自分自身を呼び出す\n        setTimeout(this.time_goes, this.player.speed)\n      },\n\n      MoveRight() {\n        console.log(\"右移動\")\n        //移動処理\n        this.$set(this.player, \"left\", 200)\n      },\n      MoveLeft() {\n        console.log(\"左移動\")\n        this.$set(this.player, \"left\", 40)\n      },\n      MoveCenter() {\n        console.log(\"中央移動\")\n        this.$set(this.player, \"left\", 120)\n      }\n\n\n    }\n\n\n\n  }\n</script>\n\n<style scoped>\n  .player {\n    position: absolute;\n\n  }\n\n  .fish {\n    position: absolute;\n\n  }\n\n\n  .cell {\n    /* width, height, marginは消す */\n    background: gray;\n  }\n\n\n\n  .title {\n    text-align: center;\n  }\n\n  .game {\n    margin: 0 auto;\n    padding: 0;\n\n    width: 300px;\n    height: 500px;\n\n  }\n\n  /* グリッドレイアウト */\n  .stage {\n    /* display: flex; */\n    /* justify-content: space-between; */\n\n    position: relative;\n\n    width: 300px;\n    height: 300px;\n\n    display: grid;\n\n    /* (列数, 幅) */\n    grid-template-columns: repeat(15, 20px);\n    /* (行数, 高さ) */\n    grid-template-rows: repeat(30, 10px);\n\n    /* width: 600px;\n    height: 300px; */\n\n    /* (列数, 幅) */\n    /* grid-template-columns: repeat(30, 20px); */\n    /* (行数, 高さ) */\n    /* grid-template-rows: repeat(60, 10px); */\n\n\n\n\n    background-color: skyblue;\n    /* text-align: center; */\n\n    margin: 0 auto;\n    /* padding-right: 5%;\n    padding-left: 5%; */\n    padding: 0;\n\n    /* text-align: center; */\n    /* padding-bottom: 100px; */\n    /* padding-right: 20%;\n    padding-left: 20%; */\n    /* display: flex;\n    flex-wrap: wrap; */\n    /* justify-content: space-between; */\n    /* justify-content: center; */\n\n    /* padding-right: 20%;\n    padding-right: 20%;\n    \n    text-align: center;\n    display: grid;\n    grid-template-columns: repeat(100, 10px);\n    /* 2列 幅30px */\n    /* 2行 高さ30px */\n    /* grid-gap: 2px; */\n    /* マス同士の隙間2px */\n\n    /* justify-items: center;\n    align-items: center; */\n  }\n\n  .controller {\n    width: 100px;\n    height: 100px;\n    /* padding-right: 20px;\n    padding-left: 20px; */\n\n\n  }\n\n  .line-box {\n    width: 200px;\n    /* heght: 100%;  */\n    background-color: blue;\n    text-align: center;\n  }\n\n  /* 全てのマス */\n  .line {\n    /* width, height, marginは消す */\n    /* background: blue; */\n    /* width: 4%;\n    margin-right: 16%;\n    margin-left: 16%; */\n\n    background: skyblue;\n    /* width: 10%;\n    margin-right: 45%;\n    margin-left: 45%;\n    height: 100%; */\n    /* text-align: center; */\n  }\n\n  .tap {\n    background: orangered;\n  }\n</style>"]}]}